URxvt*font: xft:Hack:antialias=true:size=10.5
URxvt*boldFont: xft:Hack:style=bold:antialias=true:size=10.5
URxvt*italicFont: xft:Hack:style=italic:antialias=true:size=10.5
URxvt*boldItalicFont: xft:Hack:style=bolditalic:antialias=true:size=10.5

URxvt*letterSpace: -2

Rxvt.scrollBar: false
Rxvt.saveLines: 65535

