auracle() {
    /usr/bin/auracle -C ~/aur "$@"
}

# Function to get tags from Docker Hub. Usage example:
# `dtags localstack/localstack`
dtags () {
    local image="${1}"
    wget -q https://registry.hub.docker.com/v1/repositories/"${image}"/tags -O - \
        | tr -d '[]\" ' | tr '}' '\n' | awk -F: '{print $3}'
}